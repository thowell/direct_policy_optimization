<?xml version="1.0"?>

<robot name="FloatingBaseDoublePendulum">
	<link name="world_link">
	  </link>

	<link name="x_link">
	<inertial>
	<origin xyz="0 0 0" rpy="0 0 0" />
	<mass value="1e-16" />
	<inertia ixx="1e-16" ixy="0" ixz="0" iyy="1e-16" iyz="0" izz="1e-16" />
	</inertial>
	</link>

	<link name="z_link">
	<inertial>
	<origin xyz="0 0 0" rpy="0 0 0" />
	<mass value="1e-16" />
	<inertia ixx="1e-16" ixy="0" ixz="0" iyy="1e-16" iyz="0" izz="1e-16" />
	</inertial>
	</link>

  <link name="body">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1" />
      <inertia ixx="1" ixy="0" ixz="0" iyy="0.083" iyz="0" izz="1" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
         <cylinder length="1.0" radius=".05" />
      </geometry>
      <material name="red">
	       <color rgba="1 0 0 1" />
      </material>
    </visual>
  </link>

<joint name="virtual_x" type="prismatic">
    <parent link="world_link" />
    <child link="x_link" />
    <origin xyz="0 0 0" />
    <axis xyz="1 0 0" />
		<limit velocity="1e16" effort="1e16"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

	<joint name="virtual_z" type="prismatic">
    <parent link="x_link" />
    <child link="z_link" />
    <origin xyz="0 0 0" />
    <axis xyz="0 0 1" />
		<limit velocity="1e16" effort="1e16"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

<joint name="virtual_rot" type="revolute">
    <parent link="z_link" />
    <child link="body" />
    <origin xyz="0 0 0" />
    <axis xyz="0 1 0" />
		<limit velocity="1e16" effort="1e16"/>
    <dynamics damping="0.1" friction="0.0"/>
  </joint>
</robot>
